			}
			--seconds;
			setTimeout("LoadingProgress("+seconds+");", 1000);
		}
		else{
			$("#proceeding_img")[0].style.width = "99%";
			$("#proceeding_img_text")[0].innerHTML = "99%";
			y = 0;
		}
	}
}

